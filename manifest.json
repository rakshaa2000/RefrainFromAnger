{ "manifest_version": 2,
  "name": "Refrain From Anger",
"description": "Refrain From Anger",
 "version": "1",
 "content_scripts":[
    {
      "matches":[
       "<all_urls>"
      ],
      "js": ["main.js"]
    }
  ],

  "author": "Bailey Luu",
  "content_security_policy":  
    "script-src 'self' https://apis.google.com"
, 

"browser_action": {  
      "default_title": "Refrain From Anger", 
       "default_popup": "main.html"}
}
  